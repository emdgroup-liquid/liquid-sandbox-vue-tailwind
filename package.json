    "@types/node": "^18.11.12",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "@vue/tsconfig": "^0.1.3",
    "@vue/compiler-sfc": "^3.2.41",
    "autoprefixer": "^10.4.0",
    "eslint": "^8.26.0",
    "eslint-plugin-vue": "^9.3.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.3.11",
    "prettier": "^2.7.1",
    "rollup-plugin-copy": "^3.4.0",
    "tailwindcss": "^2.0.3",
    "typescript": "^4.8.2",
    "vite": "^4.0.0",
    "vue-tsc": "^1.0.12"
